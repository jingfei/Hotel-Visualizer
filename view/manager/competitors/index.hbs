<html>
  <head>
		{{> head}}
		<link rel="stylesheet" href="/css/framework/jquery.dataTables.min.css">
		<link rel="stylesheet" href="/css/competitors.css">
  </head>
  <body>
		{{> header-manager }}
		<div id="page-wrapper">
			<div class="row">
				<div class="col-md-12">
					<h1 class="page-header">Competitors' information</h1>
				</div> 
			</div> <!-- /.row -->
			<div class="row">
				<div class="col-md-12">
					<h2>Choose one region</h2>
				</div> 
				<div class="col-md-12" id="regional-option"></div> 
			</div> <!-- /.row -->
			<div class="row"> 
				<div class="col-md-12">
					<table width="100%" class="table table-striped table-bordered table-hover" id="dataTables">
						<thead>
							<tr>
								<th>Name</th>
								<th>Address</th>
								<th>Rank</th>
							</tr>
						</thead>
						<tbody id="tableBody">
						</tbody>
					</table>
				</div>
			</div> <!-- /.row -->
		</div>
		<!-- /#page-wrapper -->
		{{> footer }}
		<script src="/js/framework/jquery.dataTables.min.js"></script>
		<script src="/js/competitors.js"></script>
  </body>
</html>
